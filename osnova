from os import system
v = int(input(' A–ª–≥–µ–±—Ä–∞ - 1 \n –ì–µ–æ–º–µ—Ç—Ä–∏—è - 2 \n –•–∏–º–∏—è - 3 \n –§–∏–∑–∏–∫–∞ - 4 \n –ö–∞–ª—å–∫—É–ª—è—Ç–æ—Ä - 5 \n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
if v == 1:
    system('clear')
    alg = int(input(' –°—Ç–µ–ø–µ–Ω—å, –∫–æ—Ä–µ–Ω—å —á–∏—Å–ª–∞ –∏ —Ç.–¥. - 1 \n –§–æ—Ä–º—É–ª—ã - 2 \n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
else:
    print('ok')
if v == 1 and alg == 1:
    system('clear')
    algv = int(input(' –°—Ç–µ–ø–µ–Ω—å, –∫–æ—Ä–µ–Ω—å –∏ —Ç.–¥. - 1 \n –§–∞–∫—Ç–æ—Ä–∏–∞–ª, —Å—É–±—Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª  –∏ —Ç.–¥. - 2 \n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
    system('clear')
    if algv == 1:
        a = float(input('–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
        system('clear')
        k2 = a*a
        k3 = a*a*a
        k4 = a*a*a*a
        k5 = a*a*a*a*a
        k6 = a*a*a*a*a*a
        k7 = a*a*a*a*a*a*a
        k8 = a*a*a*a*a*a*a*a
        k9 = a*a*a*a*a*a*a*a*a
        k10 = a*a*a*a*a*a*a*a*a*a
        print('_____.....:::::–°—Ç–µ–ø–µ–Ω–∏:::::....._____')
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤–æ –≤—Ç–æ—Ä–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k2))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ —Ç—Ä–µ—Ç—å–µ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k3))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ —á–µ—Ç–≤–µ—Ä—Ç–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k4))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ –ø—è—Ç–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k5))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ —à–µ—Å—Ç–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k6))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ —Å–µ–¥—å–º–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k7))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ –≤–æ—Å—å–º–æ–π—Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k8))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ –¥–µ–≤—è—Ç–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k9))
        print('–ß–∏—Å–ª–æ ' +str(a) + ' –≤ –¥–µ—Å—è—Ç–æ–π —Å—Ç–µ–ø–µ–Ω–∏ —ç—Ç–æ ' +str(k9))
        print('_____________________________________')
        print('___...:::–ö–≤–∞–¥—Ä–∞—Ç–Ω—ã–π –∫–æ—Ä–µ–Ω—å:::...___')
        from math import sqrt
        ko = sqrt(a)
        print('–ö–≤–∞–¥—Ä–∞—Ç–Ω—ã–π –∫–æ—Ä–µ–Ω—å —á–∏—Å–ª–∞ ' +str(a) + ' —Ä–∞–≤–µ–Ω ' +str(ko))
        print('_____________________________________')
    else:
        print()
    if algv == 2:
        a1 = int(input('–í–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ: '))
        system('clear')
        print('____....::::–§–∞–∫—Ç–æ—Ä–∏–∞–ª::::....____')
        f = 1
        for i in range(2, a1+1):
            f *= i
        print('–§–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞ ' +str(a1) + ' —Ä–∞–≤–µ–Ω ' +str(f))
        def Fact(a1):
            if a1 < 2:
                return 1
            return a1*Fact(a1-1)
        subf = (Fact(a1)+1)//2.718281828459045
        print('–°—É–±—Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞ ' +str(a1) + ' —Ä–∞–≤–µ–Ω ' +str(subf))
        df = 1 if a1 % 2 else 2
        for i in range(df + 2, a1 + 1, 2):
            df *= i
        print('–î–≤–æ–π–Ω–æ–π —Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞ ' +str(a1) + ' —Ä–∞–≤–µ–Ω ' +str(df))
        def boost_hyperfactorial(num):
            val = 1;
            for i in range(1,num+1):
                for j in range(1,i+1): 
                    val *= i;
            return val;
        num = a1;
        print('–ì–∏–ø–µ—Ä—Ñ–∞–∫—Ç–æ—Ä–∏–∞–ª —á–∏—Å–ª–∞ ' +str(a1) + ' —Ä–∞–≤–µ–Ω ' +str(boost_hyperfactorial(num)));
        print('_____________________________________')
    else:
        print()
elif v == 1 and alg == 2:
    system('clear')
