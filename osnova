    if va == 1:
        system('clear')
        print('–§–æ—Ä–º—É–ª–∞ –∫–≤–∞–¥—Ä–∞—Ç–∞ —Å—É–º–º—ã: \n (a+b)^2 = a^2 + 2ab + b^2')
        print('–§–æ—Ä–º—É–ª–∞ –∫–≤–∞–¥—Ä–∞—Ç–∞ —Ä–∞–∑–Ω–æ—Å—Ç–∏: \n (a-b)^2 = a^2 - ab + b^2')
        print('–§–æ—Ä–º—É–ª–∞ –∫—É–±–∞ —Å—É–º–º—ã: \n (a+b)^3 = a^3 + 3a^3 b + 3ab^3 + b^3')
        print('–§–æ—Ä–º—É–ª–∞ –∫—É–±–∞ —Ä–∞–∑–Ω–æ—Å—Ç–∏: \n (a+b)^3 = a^3 - 3a^3 b - 3ab^3 - b^3')
        print('–§–æ—Ä–º—É–ª–∞ —Ä–∞–∑–Ω–æ—Å—Ç–∏ –∫–≤–∞–¥—Ä–∞—Ç–æ–≤: \n a^2 - b^2 = (a-b)*(a+b)')
        print('–§–æ—Ä–º—É–ª–∞ —Å—É–º–º—ã –∫—É–±–æ–≤: \n a^3 + b^3 = (a+b)*(a^2 - ab + b^2)')
        print('–§–æ—Ä–º—É–ª–∞ —Ä–∞–∑–Ω–æ—Å—Ç–∏ –∫—É–±–æ–≤: \n a^3 - b^3 = (a-b)*(a^2 + ab + b^2)')
    elif va == 2:
        system('clear')
        print('–§–æ—Ä–º—É–ª–∞ –∫–≤–∞–¥—Ä–∞—Ç–Ω–æ–≥–æ —É—Ä–∞–≤–Ω–µ–Ω–∏—è:')
        print('ax^2 + bx + c = 0')
    elif va == 3:
        system('clear')
        print('D = b^2 - 4ac')
    elif va == 4:
        system('clear')
        print('–§–æ—Ä–º—É–ª–∞ –∫–≤–∞–¥—Ä–∞—Ç–Ω–æ–≥–æ —É—Ä–∞–≤–Ω–µ–Ω–∏—è, –ø—Ä–∏ D > 0:')
        print('x1 = ‚àíb + ‚àöD    x2 = ‚àíb ‚àí ‚àöD')
        print('        2a              2a')
    else:
