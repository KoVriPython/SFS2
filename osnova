elif v == 3:
    system('clear')
    him = int(input(' –£–∑–Ω–∞—Ç—å –∫–æ–ª-–≤–æ e, p, n –≤ –∞—Ç–æ–º–µ - 1 \n –†–µ—à–∏—Ç—å –∑–∞–¥–∞—á—É - 2\n –ü–æ–º–æ—â—å –≤ —Å–æ—Å—Ç–∞–≤–ª–µ–Ω–∏–µ —É—Ä–∞–≤–Ω–µ–Ω–∏—è - 3\n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
    if him == 1:
        system('clear')
        pn = int(input(' –í–≤–µ–¥–∏—Ç–µ –ø–æ—Ä—è–¥–∫–æ–≤—ã–π –Ω–æ–º–µ—Ä: '))
        system('clear')
        am = int(input(' –ù–µ –∑–∞–±—É–¥—Ç–µ –æ–∫—Ä—É–≥–ª–∏—Ç—å –¥–æ —Ü–µ–ª—ã—Ö \n –í–≤–µ–¥–∏—Ç–µ –∞—Ç–æ–º–Ω—É—é –º–∞—Å—Å—É: '))
        himp = pn
        himn = am - pn
        hime = pn
        system('clear')
        print(' –≠–ª–µ–º–µ–Ω—Ç —Å –∞—Ç–æ–º–Ω–æ–π –º–∞—Å—Å–æ–π ' +str(am) + ' –∏–º–µ–µ—Ç ' +str(himp) + ' –ø—Ä–æ—Ç–æ–Ω–æ–≤(p) ' +str(himn) + ' –Ω–µ–π—Ç—Ä–æ–Ω–æ–≤(n) ' +str(hime) + ' —ç–ª–µ–∫—Ç—Ä–æ–Ω–æ–≤(e)')
    elif him == 2:
        system('clear')
        vhim = int(input(' –ß–∏—Å–ª–æ –º–æ–ª–µ–∫—É–ª –≤ –∫–æ–ª-–≤–µ –• –º–æ–ª—å - 1 \n –ö–æ–ª-–≤–æ –º–æ–ª—å –≤ —ç–ª–µ–º–µ–Ω—Ç–µ —Å–æ–¥–µ—Ä–∂–∞—â–µ–º –• —á–∞—Å—Ç–∏—Ü - 2\n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: ')) #–≤—ã–±–æ—Ä —Ç–∏–ø–∞ –∑–∞–¥–∞—á–∏
        if vhim == 1:
            system('clear')
            hz1 = float(input(' –í–≤–µ–¥–∏—Ç–µ –∫–æ–ª-–≤–æ –º–æ–ª—å: '))
            ohz1 = round(hz1 * 6.02, 1)
            system('clear')
            print(' –í ' +str(hz1) + ' –º–æ–ª—å, —Å–æ–¥–µ—Ä–∂–∏—Ç—Å—è ' +str(ohz1) + '*10¬≤¬≥ –º–æ–ª–µ–∫—É–ª')
        elif vhim ==2:
            system('clear')
            hz2 = float(input('–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ —á–∞—Å—Ç–∏—Ü (–±–µ–∑ *10¬≤¬≥): '))
            ohz2 = round(hz2 / 6.02, 1)
            system('clear')
            print(str(ohz2) + ' –º–æ–ª—å –≤ —ç–ª–µ–º–µ–Ω—Ç–µ —Å–æ–¥–µ—Ä–∂–∞—â–µ–º ' +str(hz2) + '*10¬≤¬≥ —á–∞—Å—Ç–∏—Ü')
        else:
