elif v == 2:
    system('clear')
    geom = int(input(' –ù–∞–π—Ç–∏ –ø–ª–æ—â–∞–¥—å - 1 \n –§–æ—Ä–º—É–ª—ã - 2 \n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
    if geom == 1:
        system('clear')
        nplo = int(input(' –ù–∞–π—Ç–∏ –ø–ª–æ—â–∞–¥—å 4-10 —É–≥–æ–ª—å–Ω–∏–∫–∞ - 1 \n –ù–∞–π—Ç–∏ –ø–ª–æ—â–∞–¥—å –∫—Ä—É–≥–∞ - 2 \n –ù–∞–π—Ç–∏ –ø–ª–æ—â–∞–¥—å –æ–≤–∞–ª–∞ - 3\n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
        if nplo == 1:
            system('clear')
            plo = int(input(' –í–≤–µ–¥–∏—Ç–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —É–≥–ª–æ–≤ –≤ —Ñ–∏–≥—É—Ä–µ (4-10) \n –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: '))
            if plo == 4:
                system('clear')
                s41 = float(input(' –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                s42 = float(input(' –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 2 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o4 = s41*s42
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å —á–µ—Ç—ã—Ä–µ—Ö—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o4) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 5:
                system('clear')
                s5 = int(input(' –ï—Å–ª–∏ –ø—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o5 = (5 * s5*s5) / 2.906170
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å –ø—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o5) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 6:
                system('clear')
                from math import sqrt
                s61 = float(input(' –ï—Å–ª–∏ —à–µ—Å—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                fo6 = 3 * sqrt(3)* (s61*s61) / 2
                o6 = round(fo6,8)
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å —à–µ—Å—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o6) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 7:
                system('clear')
                s7 = float(input(' –ï—Å–ª–∏ —Å–µ–º–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o7 = (7 * s7*s7) / 1.926298
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å —Å–µ–º–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o7) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 8:
                system('clear')
                s8 = float(input(' –ï—Å–ª–∏ –≤–æ—Å—å–º–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o8 = (8*s8*s8) / 1.656854
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å –≤–æ—Å—å–º–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o8) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 9:
                system('clear')
                s9 = float(input(' –ï—Å–ª–∏ –¥–µ–≤—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o9 = (9*s9*s9) / 1.455880
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å –¥–µ–≤—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o9) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            elif plo == 10:
                system('clear')
                s10 = float(input(' –ï—Å–ª–∏ –¥–µ—Å—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π \n –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É 1 —Å—Ç–æ—Ä–æ–Ω—ã: '))
                o10 = (10*s10*s10) / 1.299680
                system('clear')
                print(' –ü–ª–æ—â–∞–¥—å –¥–µ—Å—è—Ç–∏—É–≥–æ–ª—å–Ω–∏–∫–∞ —Ä–∞–≤–Ω–∞ ' +str(o10) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
            else:
                system('clear')
                print (' –í—ã –≤–≤–µ–ª–∏ –Ω–µ –≤–µ—Ä–Ω–æ–µ —á–∏—Å–ª–æ')
        elif nplo == 2:
            system('clear')
            ss1 = float(input('–í–≤–µ–¥–∏—Ç–µ —Ä–∞–¥–∏—É—Å –æ–∫—Ä—É–∂–Ω–æ—Å—Ç–∏: '))
            fss1 = 3.1415 * (ss1*ss1)
            fss2 = round(fss1, 5)
            system('clear')
            print(' –ü–ª–æ—â–∞–¥—å –æ–∫—Ä—É–∂–Ω–æ—Å—Ç–∏ —Ä–∞–≤–Ω–∞ ' +str(fss2) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
        elif nplo == 3:
            system('clear')
            sss = float(input(' –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –º–∞–ª–æ–≥–æ —Ä–∞–¥–∏—É—Å–∞: '))
            sss1 = float(input(' –í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –±–æ–ª—å—à–æ–≥–æ —Ä–∞–¥–∏—É—Å–∞: ')) 
            fsss1 = 3.1415 * sss * sss1
            fsss2 = round(fsss1, 5)
            system('clear')
            print('–ü–ª–æ—â–∞–¥—å –æ–≤–∞–ª–∞ —Ä–∞–≤–Ω–∞ ' +str(fsss2) + ' —Å–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö')
        else:
            system('clear')
            print (' –í—ã –≤–≤–µ–ª–∏ –Ω–µ –≤–µ—Ä–Ω–æ–µ —á–∏—Å–ª–æ')
    elif geom == 2:
        system('clear')
        print('	ùïÑ‚Ñçùîº –ìùîºùïÜùïÑùîºùïã‚Ñô–ò–Ø \n          ‚Ñçùîº ‚Ñçùïê–ñ‚Ñçùî∏')
    else:
